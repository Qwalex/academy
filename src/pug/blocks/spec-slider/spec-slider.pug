mixin spec-slider
    .spec-slider
        .spec-slider__swiper.swiper.js-spec-slider
            .swiper-wrapper
                block
            .js-spec-slider-prev.spec-slider__nav.spec-slider__nav--prev: i.icm.icm-arrow-left
            .js-spec-slider-next.spec-slider__nav.spec-slider__nav--next: i.icm.icm-arrow-right